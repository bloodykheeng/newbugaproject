</select><br>
 </div>
  
 <div class="form-group">
    <label for="username1">enter spare name</label>
    <input name="sparename" type="text" class="form-control" placeholder="enter spare name">
  </div>
<div class="form-group">
    <label for="username1">enter spare amount</label>
    <input name="spareamount" type="number" class="form-control" placeholder="enter spare amount">
  </div>
  <div class="form-group">
    <label for="username1">enter spare rate</label>
    <input name="sparetax" type="text" class="form-control" placeholder="enter spare rate">
  </div>
  <div class="form-group">
    <label for="username1">enter spare price</label>
    <input name="spareprice" type="number" class="form-control" placeholder="enter spare price">
