<div>
  <div style="color:white; display:flex;  justify-content:center;"><p>GRAPH SHOWING TOP 30 REQUESTED SPARES</p></div>
  <canvas id="myChart" height="500px" width="500px"></canvas>
</div>
<br><br>




<div  class="table-responsive">
 <table style="max-height:90%" class="table" style="white-space:nowrap;">
  <caption>LIST OF SPARE USAGE</caption>
  <thead class="thead-dark" >

    <tr>

    <th scope="col">id</th>
      <th scope="col">Spare name</th>

   <th scope="col">Spare quantity</th>

 </tr>

  </thead>

  <tbody>

<% 

if(rows.length > 0){  
var id = 1;
  rows.forEach( (row) => {
%>
    <tr>
   
      <td><%= id %></td>

      <td><%= row.sparename %></td>

      <td><%= row.spare_quantity %></td>

       </tr>

<%  id++; }) }else{%>
<div> THERE IS NO DATA TO PRESENT</div>

 <% }%>
 
  </tbody>
</table>
</div>


		</div>
	</div>
	</div>


		
	<div class="clearfix"></div>
</div>


<script>

//var name = new Array()
let sparename = <%- sparename %>
//const name2 = JSON.parse(name)
let sparequantity = <%- sparequantity %>

/*const labels = [
  'January',
  'February',
  'March',
  'April',
  'May',
  'June',
]; 
*/
 // === include 'setup' then 'config' above ===
const data = {
  labels: sparename,
  datasets: [{
    label: 'Table showing most used 30 spares',
    backgroundColor: 'rgb(255, 99, 132)',
    borderColor: 'rgb(255, 99, 132)',
    //data: [0, 10, 5, 2, 20, 30, 45],
  data: sparequantity ,
  }]
};


const config = {
  type: 'bar',
  data,
  options: {}
};
  


  var myChart = new Chart(
    document.getElementById('myChart'),
    config
  );
  

  
</script>



<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
